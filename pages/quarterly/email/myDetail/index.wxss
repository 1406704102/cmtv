/* pages/quarterly//email/detail/index.wxss */
/* pages/slowread/slowread.wxss */

page {
    background: #f6f6f6;
}

.zmui-writer {
    margin: 30rpx 40rpx 0 40rpx;
    padding: 40rpx;
    background: #FFFFFF;
    box-shadow: 0 2rpx 9rpx 1rpx rgba(229, 229, 229, 0.7);
    border-radius: 25rpx;
    overflow: hidden;
}

.zmui-user-to {
    position: relative;
    padding-top: 100rpx;
    display: flex;
}

.zmui-user-up {
    position: absolute;
    left: 0;
    top: 50rpx;
    display: flex;
}

.zmui-user-up-text {
    padding-top: 75rpx;
    font-size: 32rpx;
    color: #ababab;
}

.zmui-user-up-on {
    color: #FF7E71;
}

.zmui-user-up-img {
    width: 123rpx;
    height: 116rpx;
}

.zmui-content {
    font-size: 32rpx;
    line-height: 72rpx;
}

.zmui-content img {
    max-width: 100%;
}

.zmui-user-width {
    flex: 1;
}

.zmui-user-to-avatar-img {
    width: 42rpx;
    height: 42rpx;
    border-radius: 50%;
}

.zmui-user-to-name {
    padding-left: 20rpx;
    line-height: 42rpx;
    font-size: 30rpx;
    color: #0F0D0B;
}

.zmui-user-time {
    text-align: right;
    font-size: 32rpx;
    color: #666666;
}

/* zmui-title */
.zmui-replay-li {
    display: flex;
    margin: 40rpx 40rpx 0 40rpx;
    height: 90rpx;
    background: #FFFFFF;
    overflow: hidden;
}

.zmui-title-icon {
    line-height: 90rpx;
    padding: 0rpx 40rpx;
}

.zmui-title-icon-img {
    width: 38rpx;
    height: 22rpx;
}

.zmui-title-name {
    padding-right: 20rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 32rpx;
    color: #0F0D0B;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* zmui-user-infos */
.zmui-user-infos {
    display: flex;
    height: 130rpx;
}

.zmui-user-name {
    padding-left: 20rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #F86564;
}

.zmui-user-avatar-img {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.zmui-btn {
    display: flex;
    margin: 60rpx 40rpx 100rpx;
}

.zmui-btn-line {
    flex: 1;
}

/* zmui-writer-button */
.zmui-writer-button-replay {
    width: 312rpx !important;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    font-size: 36rpx;
    color: #F86564;
    background: #FFFFFF;
    border-radius: 40rpx;
}

.zmui-writer-button {
    width: 312rpx !important;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
    font-size: 36rpx;
    color: #FFFFFF;
}
.zmui-writer-button2 {
    width: 352rpx !important;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
    font-size: 36rpx;
    color: #FFFFFF;
}
