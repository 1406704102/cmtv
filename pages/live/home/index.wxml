        <view style="width: 710rpx;" class="radius">
            <swiper class="screen-swiper radius" easing-function="easeOutCubic" indicator-dots="true" circular="true"
                    autoplay="true" interval="5000" duration="500" indicator-active-color="#F86465">
                <swiper-item wx:for="{{swiperList}}" wx:key class="radius" catch:tap="imageClick"
                             data-url="{{item.bannerJumpUrl}}">
                    <image src="{{item.bannerUrl}}" mode="aspectFill" class="radius" catch:tap=""></image>
                </swiper-item>
            </swiper>
        </view>
    </view>
