                </view>
                <view class="flex justify-center flex-direction margin-left-sm">
                    <view>今日推荐</view>
                </view>
            </view>
            <navigator wx:for="{{recommendToday}}" wx:key
                       url="/pages/my/userDetail/index?id={{item.id}}&tabShow=1&tabCur=1"
                       class="shadow bg-white margin-sm flex justify-between radius-l "
                       style="padding: 36rpx 35rpx 44rpx 33rpx;">

                <view class="flex justify-start">

                    <view class='text-center'>
                        <view class="cu-avatar xl round {{item.living === 2 ? 'border-F86564':'border-428597'}}"
                              style="background-image:url({{item.avatar}});">

                            <view wx:if="{{item.living === 2}}">
                                <image wx:if="{{item.livingK !== 'null'}}" src="/images/Kwai.png"
                                       style="width: 34rpx;height: 34rpx;margin-top: 110rpx;margin-left: 110rpx;"></image>
                                <image wx:elif="{{item.livingD !== 'null'}}" src="/images/TikTop2.png"
                                       style="width: 34rpx;height: 34rpx;margin-top: 110rpx;margin-left: 110rpx;"></image>
                                <image wx:else src="/images/live.png"
                                       style="width: 34rpx;height: 34rpx;margin-top: 110rpx;margin-left: 110rpx;"></image>
                            </view>

                        </view>
                    </view>
                    <view class='text-left justify-between flex flex-direction padding-left-sm margin-top-xs '
                          style="height: 120rpx;width: 330rpx;">
                        <view class="flex justify-start text-lg text-bold">
                            {{item.nickname}}
                            <view wx:if="{{item.subing}}">
                                <view class="cu-tag radius bg-428597 sm text-white text-xs margin-left-xs">已订阅</view>
                            </view>
                        </view>


                        <view class="text-sm margin-top-xs text-cut" style="color: #6a6a6a" wx:if="{{item.liveName}}">
                            {{item.liveName}}
                        </view>
                        <view class="text-sm margin-top-xs text-cut" style="color: #6a6a6a" wx:else>{{item.signature}}
                        </view>
                        <view class="text-xs text-gray">{{item.fansNum}} 订阅</view>
                    </view>
                </view>

                <view class="text-center flex justify-between flex-direction" style="margin-top: 38rpx">
                    <view wx:if="{{item.living === 2}}">
                        <button class="cu-btn bg-F86564 shadow text-white" catch:tap="showModal"
                                data-target="DialogModal2"
                                data-item="{{item}}">观看直播
                        </button>
                    </view>
                    <view wx:if="{{item.living === 1 && item.remind===0}}">
                        <button class="cu-btn text-0090a1 " catch:tap="remind" data-item="{{item.liveId}}"
                                style="background:#ffffff;border: #428597 solid 1rpx">直播提醒
                        </button>
                    </view>
                    <!--                    <button class="cu-btn round line{{bordersize?bordersize:''}}-{{item.name}} {{shadow?'shadow':''}}">{{item.title}}</button>-->
                    <view wx:if="{{item.remind===1}}">
                        <button class="cu-btn bg-ffb554 shadow text-white" catch:tap="reminded">等待直播
                        </button>
                    </view>


                    <view wx:if="{{item.living === 0}}">
                        <button class="cu-btn bg-428597 shadow text-white" catch:tap="showModal2"
                                data-target="DialogModal3"
                                data-replay="{{item.replay}}">直播回放
                        </button>
                    </view>
                    <view wx:if="{{item.living === -1}}">
                        <button class="cu-btn bg-69dd96 shadow text-white" catch:tap="wVideo" data-item="{{item}}">观看视频
                        </button>
